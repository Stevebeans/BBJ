    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.1.8"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "flowbite": "^1.5.3",
    "jquery": "^3.6.0",
